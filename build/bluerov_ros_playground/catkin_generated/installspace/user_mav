#!/usr/bin/env python2

import rospy

from bluerov.Joystick import Control
from bluerov.Control import Code1
from bluerov.Control_Autonomou import Code

def main():
    try:
        rospy.init_node('user_node', log_level=rospy.DEBUG)
    except rospy.ROSInterruptException as error:
        print('pubs error with ROS: ', error)
        exit(1)
    code4 = Control()
    code4.run()

if __name__ == "__main__":
    main()
